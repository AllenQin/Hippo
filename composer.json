{
    "name": "Hippo",
    "description": "a php framework",
    "keywords": ["php framework", "Hippo"],
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "monolog/monolog": "^1.19",
        "pimple/pimple": "^3.0",
        "phpseclib/phpseclib": "^2.0",
        "broadway/broadway": "^0.9.0",
        "nesbot/carbon": "^1.21",
        "league/csv": "^8.1",
        "tightenco/collect": "^5.3",
        "illuminate/support": "^5.7",
        "guzzlehttp/guzzle": "~6.0",
        "symfony/event-dispatcher": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "mockery/mockery": "^0.9.5",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Services\\": "application/services",
            "App\\Helpers\\": "application/helpers",
            "App\\Defines\\": "application/defines",
            "App\\Library\\Core\\": "application/library/Core"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
