{
    "name": "Hippo",
    "description": "a php framework",
    "keywords": ["php framework", "Hippo"],
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "monolog/monolog": "^1.19",
        "pimple/pimple": "^3.0",
        "phpseclib/phpseclib": "^2.0",
        "broadway/broadway": "^0.9.0",
        "nesbot/carbon": "^1.21",
        "league/csv": "^8.1",
        "tightenco/collect": "^5.3",
        "illuminate/support": "5.5.*",
        "guzzlehttp/guzzle": "~6.0",
        "symfony/event-dispatcher": "^4.2",
        "catfan/medoo": "^1.6",
        "chrisboulton/php-resque": "^1.2",
        "filp/whoops": "^2.3",
        "illuminate/database": "5.5.*",
        "illuminate/events": "5.5.*",
        "doctrine/dbal": "^2.9",
        "symfony/var-dumper": "^4.2",
        "symfony/console": "^4.2",
        "illuminate/validation": "5.5.*",
        "doctrine/migrations": "^2.0",
        "psy/psysh": "^0.9.9",
        "delight-im/cookie": "^3.1",
        "firebase/php-jwt": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "mockery/mockery": "^0.9.5",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Services\\": "application/services",
            "App\\Helpers\\": "application/helpers",
            "App\\Defines\\": "application/defines",
            "App\\Library\\Core\\": "application/library/Core",
            "App\\Models\\":"application/models",
            "Command\\":"application/command/",
            "Db\\Types\\":"db/types/"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
